{
  "name": "brave",
  "manifest_version": 2,
  "version": "1.0",
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "http://localhost:*/about-preferences.html",
        "http://localhost:*/about-bookmarks.html",
        "http://localhost:*/about-downloads.html",
        "http://localhost:*/about-certerror.html",
        "http://localhost:*/about-passwords.html",
        "file://*/extensions/brave/about-preferences.html",
        "file://*/extensions/brave/about-bookmarks.html",
        "file://*/extensions/brave/about-downloads.html",
        "file://*/extensions/brave/about-certerror.html",
        "file://*/extensions/brave/about-passwords.html"
      ],
      "js": [
        "brave-about.js"
      ]
    },
    {
      "all_frames": true,
      "matches": ["http://*/*", "https://*/*"],
      "run_at": "document_start",
      "js": [
        "brave-default.js"
      ],
      "css": [
        "brave-default.css"
      ]
    }
  ],
  "incognito": "spanning",
  "key": ""
}
